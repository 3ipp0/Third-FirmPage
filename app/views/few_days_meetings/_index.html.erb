<p id="notice"><%= notice %></p>

<h1>Few Days Meetings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @few_days_meetings.each do |few_days_meeting| %>
      <tr>
        <td><%= few_days_meeting.name %></td>
        <td><%= few_days_meeting.start_time %></td>
        <td><%= few_days_meeting.end_time %></td>
        <td><%= link_to 'Show', few_days_meeting %></td>
        <td><%= link_to 'Edit', edit_few_days_meeting_path(few_days_meeting) %></td>
        <td><%= link_to 'Destroy', few_days_meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Few Days Meeting', new_few_days_meeting_path %>
